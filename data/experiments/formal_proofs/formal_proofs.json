  "THM2": {
    "name": "Canonical U Unitary",
    "statement": "U = \u03a6(\u03a6\u2020\u03a6)^{-1/2} satisfies U\u2020U = UU\u2020 = I",
    "status": "constructive",
    "verified": true,
    "dependencies": [
      "THM1"
    ],
    "steps": [
      {
        "step_id": 1,
        "statement": "By Theorem 1, \u03a6 is invertible \u27f9 G = \u03a6\u2020\u03a6 is Hermitian positive-definite",
        "rule": "modus_ponens",
        "references": [
          "THM1"
        ]
      },
      {
        "step_id": 2,
        "statement": "G^{-1/2} is well-defined: G = V\u039bV\u2020 \u27f9 G^{-1/2} = V\u039b^{-1/2}V\u2020",
        "rule": "spectral",
        "references": [
          "step_1"
        ]
      },
      {
        "step_id": 3,
        "statement": "U\u2020U = G^{-1/2} \u03a6\u2020\u03a6 G^{-1/2} = G^{-1/2} G G^{-1/2} = I (spectral calculus: f(G)\u00b7g(G) = (f\u00b7g)(G))",
        "rule": "algebraic",
        "references": [
          "step_1",
          "step_2"
        ]
      },
      {
        "step_id": 4,
        "statement": "N=8: ||U\u2020U - I||_F = 4.56e-15, ||UU\u2020 - I||_F = 4.48e-15",
        "rule": "numerical_cert",
